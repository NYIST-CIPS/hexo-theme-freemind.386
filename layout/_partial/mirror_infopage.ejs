<h1>域名选择</h1>
<a type="button" href=<%=config.url%> class="btn btn-default more"><%=config.url%>（自动）</a>
<a type="button" href=<%=config.url4%> class="btn btn-default more"><%=config.url4%>（IPv4）</a>
<a type="button" href=<%=config.url6%> class="btn btn-default more"><%=config.url6%>（IPv6）</a>
<a type="button" href=<%=config.urlocal%> class="btn btn-default more"><%=config.urlocal%>（校内）</a>
<hr>

<h1>常用镜像下载</h1>
<div class="sys-logo" >
    <% config.quick_mirror.forEach(function(mirrors){ %>
        <a class="btn btn-default sub-sys-logo" href="help/<%=mirrors%>" type="button">
            <div>
                <div style="width: 4rem;height: 4rem;background-repeat: no-repeat;background-position: center;background-image: url(/img/syslogo/<%=mirrors%>.svg);"></div>
                <%=mirrors%>
            </div>
        </a>
    <% }); %>
</div>
<hr>